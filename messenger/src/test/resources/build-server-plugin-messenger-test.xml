<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns              = "http://www.springframework.org/schema/beans"
       default-autowire   = "constructor"
       xmlns:xsi          = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
                             http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <!-- Utility beans -->
    <bean class="com.github.goldin.plugins.teamcity.messenger.test.infra.MessagesContextImpl"/>
    <bean class="com.github.goldin.plugins.teamcity.messenger.test.infra.TestServerPaths"/>
    <bean class="com.github.goldin.plugins.teamcity.messenger.api.MessagesUtil"/>

    <!-- Working beans -->
    <bean class="com.github.goldin.plugins.teamcity.messenger.impl.MessagesBeanImpl" name="messages-bean"/>
    <bean class="com.github.goldin.plugins.teamcity.messenger.impl.MessagesConfigurationImpl"/>
    <bean class="com.github.goldin.plugins.teamcity.messenger.impl.MessagesPersistencyImpl"/>
    <bean class="com.github.goldin.plugins.teamcity.messenger.impl.MessagesTableImpl"/>
    <bean class="com.github.goldin.plugins.teamcity.messenger.impl.UsersTableImpl"/>
</beans>
